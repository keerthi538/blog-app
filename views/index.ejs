<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs')%>
<body>
    
    <%- include('./partials/nav.ejs')%>

    <div class="blog content">
        <h2>All Questions</h2>

        <% if(blogs.length > 0){%>
            <%blogs.forEach((blog) =>{ %>
                <a class="single" href="/blogs/<%= blog.id %>">
                    <h3><%= blog.title %></h3>
                    <p><%= blog.snippet %></p>
                </a>
            <% }) %>
        <% } else{ %>

            <p>There are no questions</p>
            <% } %>
    </div>

    <%- include('./partials/footer.ejs')%>
</body>
</html>